{
  "name": "weex-frame",
  "description": "a frame for weex ~",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "keywords": [
    "weex",
    "vue"
  ],
  "scripts": {
    "init": "node build/init.js",
    "dev": "webpack --watch",
    "copy": "rm -rf platforms/android/app/src/main/assets/* && cp -vrf dist/weex/* platforms/android/app/src/main/assets&& rm -rf platforms/ios/assets/* && cp -vrf dist/weex/* platforms/ios/assets",
    "serve": "node build/ip.js && serve -p 8080",
    "build": "webpack",
    "debug": "weex-devtool"
  },
  "dependencies": {
    "vue": "^2.1.8",
    "vue-router": "^2.1.1",
    "vuex": "^2.1.1",
    "vuex-router-sync": "^4.0.1",
    "weex-vue-render": "^0.11.9"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "postcss-cssnext": "^2.9.0",
    "serve": "^1.4.0",
    "vue-loader": "^10.3.0",
    "vue-progressbar": "^0.7.1",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^1.14.0",
    "weex-devtool": "^0.2.64",
    "weex-loader": "^0.4.4",
    "weex-vue-loader": "^0.2.12"
  },
  "optionalDependencies": {
    "ios-deploy": "^1.9.0"
  }
}